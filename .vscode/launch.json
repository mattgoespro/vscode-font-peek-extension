{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}", "--disable-extensions"],
      "remoteRoot": "${workspaceFolder}",
      "debugWebviews": true,
      "preLaunchTask": "dev",
      "cwd": "${workspaceFolder}"
    }
  ]
}
